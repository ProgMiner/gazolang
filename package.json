{
    "name": "gazolang",
    "version": "1.0.0",
    "private": true,
    "repository": {
        "url": "git+https://github.com/ProgMiner/gazolang.git"
    },
    "scripts": {
        "start": "webpack serve --mode=development -d eval-source-map",
        "build": "shx mkdir -p ./build && shx cp -r ./public/* ./build && webpack --mode=production",
        "clean": "shx rm -rf ./build",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d ./build"
    },
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "css-loader": "^6.8.1",
        "gh-pages": "^5.0.0",
        "purs-loader": "^4.0.0",
        "shx": "^0.3.4",
        "ts-loader": "^9.4.3",
        "typescript": "^5.0.4",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.1",
        "webpack-dev-server": "^4.15.0"
    }
}
